PREFIX iter: <http://w3id.org/sparql-generate/iter/>
PREFIX fn: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX bs: <http://opensensingcity.emse.fr/tuba/ontology#>

GENERATE { 
    ?iri bs:name ?name .
}
ITERATOR iter:JSONPath(?message, "$.features"  ) AS ?feature
WHERE {
     BIND( fn:JSONPath(?feature, "$.properties.gid")   AS ?gid)
     BIND( fn:JSONPath(?feature, "$.properties.name")  AS ?name)
     BIND( URI( CONCAT( "http://opensensingcity.emse.fr/tuba/lyon/bicycles/", ?gid ) ) AS ?iri )
}